{
  "name": "@dope-js/core",
  "version": "0.0.11",
  "description": "React router like nextjs.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dopeJs/dope-js",
    "directory": "packages/core"
  },
  "homepage": "https://dopejs.dev",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "packageManager": "pnpm@7.12.0",
  "bin": {
    "dope": "./bin/dope.js"
  },
  "main": "./lib/index.js",
  "types": "src/index.ts",
  "files": [
    "bin",
    "lib",
    "typings.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./lib/*": "./lib/*",
    "./package.json": "./package.json",
    "./typings": {
      "types": "./typings.d.ts"
    }
  },
  "publishConfig": {
    "types": "lib/index.d.ts",
    "access": "public",
    "directory": ".dope.publish",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "rimraf ./.dope.publish && pnpm clean-publish --access public",
    "postpublish": "rimraf ./.dope.publish",
    "lint": "eslint --cache --ext .ts src/**"
  },
  "author": "John Zhang",
  "license": "MIT",
  "devDependencies": {
    "@types/http-proxy": "^1.17.9",
    "@types/react": "^18.0.20",
    "clean-publish": "^4.0.1",
    "react": "^18.2.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@mdx-js/react": "^2.1.3",
    "@mdx-js/rollup": "^2.1.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/debug": "^4.1.7",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^9.4.0",
    "debug": "^4.3.4",
    "esbuild": "^0.15.9",
    "fast-glob": "^3.2.12",
    "find-up": "^5.0.0",
    "http-proxy": "^1.18.1",
    "rollup": "^2.79.0",
    "vite": "^3.1.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-index-html": "^2.0.2",
    "vite-plugin-virtual-html": "^0.2.11",
    "winston": "^3.8.2"
  },
  "peerDependencies": {
    "react": ">=18"
  }
}

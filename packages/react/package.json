{
  "name": "@melon-js/design",
  "version": "0.0.1",
  "description": "A simple design system and UI library.",
  "repository": {
    "type": "git",
    "url": "https://github.com/melonote-app/melon-design"
  },
  "packageManager": "pnpm@7.11.0",
  "engines": {
    "node": ">=16.15.0",
    "pnpm": ">=7.4.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "main": "lib/md.cjs.js",
  "module": "lib/md.es.js",
  "browser": "lib/md.umd.js",
  "types": "src/index.ts",
  "files": [
    "theme.d.ts",
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/md.es.js",
      "require": "./lib/md.cjs.js",
      "default": "./lib/md.umd.js"
    },
    "./lib/*": "./lib/*",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "types": "lib/index.d.ts",
    "access": "public",
    "directory": "temp-package-dir",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "start": "rollup -cw",
    "build": "rimraf lib && rollup -c",
    "prepublishOnly": "rimraf ./temp-package-dir && clean-publish --access public",
    "postpublish": "rimraf ./temp-package-dir"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@floating-ui/react-dom": "^1.0.0",
    "@floating-ui/react-dom-interactions": "^0.9.3",
    "rc-dialog": "^8.9.0",
    "rc-tree": "^5.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-model": "^4.2.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "clean-publish": "^4.0.1",
    "rollup": "^2.79.0",
    "tslib": "^2.4.0"
  }
}

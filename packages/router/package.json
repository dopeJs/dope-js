{
  "name": "@melon-js/router",
  "version": "0.0.4",
  "description": "Router for melon-js.",
  "repository": {
    "type": "git",
    "url": "https://github.com/melonote-app/melon-design"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "packageManager": "pnpm@7.12.0",
  "main": "lib/router.cjs.js",
  "module": "lib/router.es.js",
  "browser": "lib/router.umd.js",
  "types": "src/index.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/router.es.js",
      "require": "./lib/router.cjs.js",
      "default": "./lib/router.umd.js"
    },
    "./lib/*": "./lib/*",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "types": "lib/index.d.ts",
    "access": "public",
    "directory": "temp-package-dir",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "rimraf lib && rollup -cw",
    "build": "rimraf lib && rollup -c && tsc-alias",
    "clean-publish": "clean-publish --access public",
    "prepublishOnly": "rimraf ./temp-package-dir && run-s build clean-publish",
    "postpublish": "rimraf ./temp-package-dir",
    "lint": "eslint --cache --ext .ts src/**"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/react": "^18.0.20",
    "clean-publish": "^4.0.1",
    "rollup": "^2.79.0",
    "tsc-alias": "^1.7.0",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "history": "^5.3.0",
    "path-to-regexp": "^6.2.1",
    "url-toolkit": "^2.2.5"
  },
  "peerDependencies": {
    "react": ">=17.0.2"
  }
}

{
  "name": "@melon-js/router",
  "version": "0.0.5",
  "description": "Router for melon-js.",
  "repository": {
    "type": "git",
    "url": "https://github.com/melonote-app/melon-design",
    "directory": "packages/router"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "packageManager": "pnpm@7.12.0",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "browser": "lib/index.umd.js",
  "types": "src/index.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.cjs.js",
      "default": "./lib/index.umd.js"
    },
    "./lib/*": "./lib/*",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "types": "lib/index.d.ts",
    "access": "public",
    "directory": ".melon.publish",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "rimraf ./.melon.publish && clean-publish --access public",
    "postpublish": "rimraf ./.melon.publish",
    "lint": "eslint --cache --ext .ts,.tsx src/**"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.20",
    "clean-publish": "^4.0.1"
  },
  "dependencies": {
    "history": "^5.3.0",
    "path-to-regexp": "^6.2.1",
    "url-toolkit": "^2.2.5"
  },
  "peerDependencies": {
    "react": ">=17.0.2"
  }
}

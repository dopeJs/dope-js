{
  "name": "@melon-js/router",
  "version": "0.0.1",
  "description": "Router for melon-js.",
  "repository": {
    "type": "git",
    "url": "https://github.com/melonote-app/melon-design"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "packageManager": "pnpm@7.12.0",
  "main": "lib/router.cjs.js",
  "module": "lib/router.es.js",
  "browser": "lib/router.umd.js",
  "types": "src/index.ts",
  "files": [
    "theme.d.ts",
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/router.es.js",
      "require": "./lib/router.cjs.js",
      "default": "./lib/router.umd.js"
    },
    "./lib/*": "./lib/*",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "types": "lib/index.d.ts",
    "access": "public",
    "directory": "temp-package-dir",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "rimraf lib && rollup -cw",
    "build": "rimraf lib && rollup -c"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.20",
    "rollup": "^2.79.0",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "history": "^5.3.0",
    "path-to-regexp": "^6.2.1",
    "url-toolkit": "^2.2.5"
  },
  "peerDependencies": {
    "react": ">=17.0.2"
  }
}

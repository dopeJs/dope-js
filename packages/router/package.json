{
  "name": "@melon-js/router",
  "version": "0.0.1",
  "description": "Router for melon-js.",
  "repository": {
    "type": "git",
    "url": "https://github.com/melonote-app/melon-design"
  },
  "packageManager": "pnpm@7.12.0",
  "engines": {
    "node": ">=16.15.0",
    "pnpm": ">=7.4.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "main": "lib/router.cjs.js",
  "module": "lib/router.es.js",
  "browser": "lib/router.umd.js",
  "types": "src/index.ts",
  "files": [
    "theme.d.ts",
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/router.es.js",
      "require": "./lib/router.cjs.js",
      "default": "./lib/router.umd.js"
    },
    "./lib/*": "./lib/*",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "types": "lib/index.d.ts",
    "access": "public",
    "directory": "temp-package-dir",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rimraf lib && rollup -c"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@melon-js/core": "workspace:*",
    "@types/react": "^18.0.20",
    "rollup": "^2.79.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-router": "^6.4.0",
    "react-router-dom": "^6.4.0"
  }
}

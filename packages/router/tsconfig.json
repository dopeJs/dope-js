{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "downlevelIteration": true,
    "rootDir": "src",
    "outDir": "lib",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*.tsx", "router.d.ts", "src/index.ts"],
  "exclude": ["lib", "node_modules"]
}
